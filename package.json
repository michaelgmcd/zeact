    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3",
    "webpack": "^3.5.4"
  }
}
